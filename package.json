    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "10.9.2",
    "tslib": "2.8.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0"
  }
